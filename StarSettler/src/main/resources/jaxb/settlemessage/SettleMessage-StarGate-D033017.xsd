                                                                                                                    </xs:restriction>
                                                                                                                </xs:simpleType>
                                                                                                            </xs:element>
                                                                                                            <xs:element name="CountryCode" minOccurs="1" maxOccurs="1">
                                                                                                                <xs:simpleType>
                                                                                                                    <xs:restriction base="xs:string">
                                                                                                                        <xs:minLength value="1"/>
                                                                                                                        <xs:maxLength value="2"/>
                                                                                                                    </xs:restriction>
                                                                                                                </xs:simpleType>
                                                                                                            </xs:element>
                                                                                                        </xs:sequence>
                                                                                                    </xs:complexType>
                                                                                                </xs:element>
                                                                                            </xs:sequence>
                                                                                        </xs:complexType>
                                                                                    </xs:element>
                                                                                </xs:sequence>
                                                                            </xs:complexType>
                                                                        </xs:element>
                                                                    </xs:sequence>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                        <xs:attribute name="line_id" use="required">
                                                            <xs:simpleType>
                                                                <xs:restriction base="xs:integer">
                                                                    <xs:totalDigits value="10"/> 
                                                                </xs:restriction>
                                                            </xs:simpleType>

<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cmpmsg="http://firstdata.com/cmpwsapi/schemas/cmpmsg" xmlns:cmpapi="http://firstdata.com/cmpwsapi/schemas/cmpapi" targetNamespace="http://firstdata.com/cmpwsapi/schemas/cmpmsg" elementFormDefault="qualified" version="2.8">
 
  <xs:complexType name="Transaction">
    <xs:sequence>
      <xs:element name="OrderNumber">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="22" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>      
      <xs:element name="Mop">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="AX" />
            <!--American Express/Optima-->
            <xs:enumeration value="BL" />
            <!--Bill Me Later-->
            <xs:enumeration value="DC" />
            <!--Diners Club-->
            <xs:enumeration value="DI" />
            <!--Discover-->
            <xs:enumeration value="DP" />
            <!--Generic PINless Debit-->
            <xs:enumeration value="GC" />
            <!--Google Checkout-->
            <xs:enumeration value="JC" />
            <!--JCB-->
            <xs:enumeration value="MC" />
            <!--MasterCard-->
            <xs:enumeration value="NP" />
            <!--NYCE PINless Debit-->
            <xs:enumeration value="PP" />
            <!--Pulse PINless Debit-->
            <xs:enumeration value="PY" />
            <!--PayPal-->
            <xs:enumeration value="SP" />
            <!--Star PINless Debit-->
            <xs:enumeration value="VI" />
            <!--Visa-->
            <xs:enumeration value="VL" />
            <!--ValueLink-->            
            <xs:enumeration value="NB" />
            <!--Private label products-->
            <xs:enumeration value="GE" />
            <!--Private label products-->
            <!-- <xs:enumeration value="SW" /> -->   <!-- comment for 2.23 MOP remove -->
            <!--UK Domestic Maestro Authentication-->
            <!-- <xs:enumeration value="ED" /> -->   <!-- comment for 2.23 MOP remove -->
            <!--EU Direct Debit-->
            <xs:enumeration value="TC" />
            <!-- Telecheck 2.11 -->
            <xs:enumeration value="MI" />
            <!-- Maestro International -->
            <xs:enumeration value="  " />  <!-- Updated for 2.25 change -->
            <!-- PLCC -->
            <xs:enumeration value="SA" />  <!-- Updated for 2.25 change -->
            <!-- START/Signature Debit -->
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AccountNumber" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="19" />
            <!-- xs:minLength value="1" /-->
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ExpirationDate" minOccurs="0">
        <!--MMYY format-->
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{4}" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DivisionNumber">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{5,10}" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Amount">
        <xs:simpleType>
          <xs:restriction base="xs:string">
             <xs:pattern value="[0-9]{1,12}" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="CurrencyCode">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{3}" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="TransactionType">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="1" />
            <!--MOTO Indicator – Single Transaction-->
            <xs:enumeration value="2" />
            <!--MOTO Indicator – Recurring Transaction-->
            <xs:enumeration value="3" />
            <!--MOTO Indicator – Installment Payment-->
            <xs:enumeration value="4" />
            <!--MOTO Indicator – Deferred Transaction-->
            <xs:enumeration value="5" />
            <!--ECI Indicator – Secure Electronic-->
            <xs:enumeration value="6" />
            <!--ECI Indicator – Non-Authenticated-->
            <xs:enumeration value="7" />
            <!--ECI Indicator – Channel Encrypted Transaction-->
            <xs:enumeration value="8" />
            <!--ECI Indicator – Non-Secure Electronic Commerce-->
            <xs:enumeration value="I" />
            <!--IVR (PINless Debit only)-->
            <xs:enumeration value="R" />
            <!--Retail Indicator -->
            <xs:enumeration value=" " />
            <!--Default space filled-->
            <xs:enumeration value="X" />
            <!--Re-Authorise transaction-->
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AccountNumberDesc" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="PAN" />
            <!--TransArmor token request-->
            <xs:enumeration value="TOK" />
            <!--TransArmor token supplied-->
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="BillPaymentIndicator" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Y" />
            <xs:enumeration value="N" />
            <xs:enumeration value=" " />
            <xs:enumeration value="A" />
            <xs:enumeration value="D" />
            <xs:enumeration value="J" />
            <xs:enumeration value="B" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ActionCode">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="AR" />
            <!--Authorization Reversal-->
            <xs:enumeration value="AU" />
            <!--Authorize-->
            <xs:enumeration value="BI" />
            <!--Balance Inquiry-->
            <xs:enumeration value="CO" />
            <!--Cash Out-->
            <xs:enumeration value="CV" />
            <!--Redemption Reversal-->
            <xs:enumeration value="DP" />
            <!--Unconditional Deposit-->
            <xs:enumeration value="DR" />
            <!--Refund Authorization Reversal-->
            <xs:enumeration value="GK" />
            <!--Get Key-->
            <xs:enumeration value="IR" />
            <!--Void of Activation (ValueLink)-->
            <xs:enumeration value="PA" />
            <!--Purchase Authorization-->
            <xs:enumeration value="PR" />
            <!--Purchase Authorization Reversal-->
            <xs:enumeration value="RA" />
            <!--Refund Auth (PINless Debit)-->
            <xs:enumeration value="RF" />
            <!--Refund-->
            <xs:enumeration value="RV" />
            <!--Refund Reversal (Value Link)-->
            <xs:enumeration value="SA" />
            <!--Add Value (Value Link)-->
            <xs:enumeration value="SD" />
            <!--Stored Value Deactivation (Value Link)-->
            <xs:enumeration value="SI" />
            <!--Stored Value Issuance (Value Link)-->
            <xs:enumeration value="VC" />
            <!--Void of Cashout (ValueLink)-->
            <xs:enumeration value="VF" />
            <!--Verification Only (Discover, MasterCard, Visa)-->
            <xs:enumeration value="VR" />
            <!--Add Value Reversal (Value Link)-->
            <xs:enumeration value="CH" />
            <!--Change (eCheck)-->
            <xs:enumeration value="DC" />
            <!--Conditional Deposit (Credit Card, Bill Me Later, PayPal, Google Checkout)-->
            <!-- <xs:enumeration value="DO" /> -->   <!-- comment for 2.23 action code  remove -->
            <!--Validate and Deposit (eCheck)-->
            <!-- removed from 2.13 spec -->
            <!--xs:enumeration value="ER" /-->
            <!--Refund (eCheck)-->
            <xs:enumeration value="VI" />
            <!--Void (eCheck)-->
            <xs:enumeration value="GT" />
            <!--Generate Token (TransArmor)-->
            <!-- <xs:enumeration value="DO" /> -->  <!-- comment for 2.23 action code  remove -->
            <!--Deposit Only - EU Direct Debit -->
            <!-- <xs:enumeration value="LO" /> -->  <!-- comment for 2.23 action code  remove -->
            <!--Validate Only - EU Direct Debit -->
            <!-- <xs:enumeration value="OR" /> --> <!-- comment for 2.23 action code  remove -->
            <!--Offsetting Refund - EU Direct Debit -->
            <!-- <xs:enumeration value="RM" /> --> <!-- comment for 2.23 action code  remove -->
            <!--Request Mandate Action - EU Direct Debit -->
            <xs:enumeration value="SC" />
            <!--Request Mandate Action - Fraud Score -->
            <xs:enumeration value="DA" />
            <!--Deposit Available (ValueLink) -->           
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

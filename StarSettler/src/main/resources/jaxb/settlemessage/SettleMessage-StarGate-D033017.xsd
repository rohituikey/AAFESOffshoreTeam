                                                                                        <xs:simpleType>
                                                                                            <xs:restriction base="xs:string">
                                                                                                <xs:minLength value="1"/>
                                                                                                <xs:maxLength value="19"/>
                                                                                            </xs:restriction>
                                                                                        </xs:simpleType>
                                                                                    </xs:element>   
                                                                                    <xs:element name="CardToken" minOccurs="1" maxOccurs="1">                
                                                                                        <xs:simpleType>
                                                                                            <xs:restriction base="xs:string">
                                                                                                <xs:minLength value="12"/>

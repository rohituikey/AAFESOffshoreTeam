                                                                                                <xs:pattern value="[0-9][0-9][0-9][0-9][0-1][0-9][0-3][0-9]"/>
                                                                                            </xs:restriction>
                                                                                        </xs:simpleType>
                                                                                    </xs:element>
                                                                                    <xs:element name="ShipDate" minOccurs="1" maxOccurs="1">
                                                                                        <xs:simpleType>
                                                                                            <xs:restriction base="xs:integer">
                                                                                                <xs:pattern value="[0-9][0-9][0-9][0-9][0-1][0-9][0-3][0-9]"/>
                                                                                            </xs:restriction>
                                                                                        </xs:simpleType>
                                                                                    </xs:element>
                                                                                    <xs:element name="SettleDate"  minOccurs="1" maxOccurs="1">
                                                                                        <xs:simpleType>
                                                                                            <xs:restriction base="xs:integer">
                                                                                                <xs:pattern value="[0-9][0-9][0-9][0-9][0-1][0-9][0-3][0-9]"/>
                                                                                            </xs:restriction>
                                                                                        </xs:simpleType>
                                                                                    </xs:element>
                                                                            
                                                                                    <xs:element name="CardReference" minOccurs="1" maxOccurs="1">
